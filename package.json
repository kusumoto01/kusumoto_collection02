    "scripts": {
        "start": "NODE_ENV=development webpack serve",
        "start:https": "NODE_ENV=development webpack serve --https",
        "start:docs": "docsify serve docs -p 9999",
        "clean": "rm -rf node_modules && npm i && npm audit fix",
        "lint": "eslint './src/**/*' --ext .ts,.tsx",
        "lint:fix": "eslint --fix './src/**/*' --ext .ts,.tsx",
        "build": "NODE_ENV=production webpack",
        "deploy": "webpack && firebase deploy --only hosting",
        "deploy:docs": "firebase deploy --only hosting:docs",
        "test": "jest --coverage",
        "storybook": "start-storybook",
        "build:storybook": "build-storybook"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "ts",
            "tsx"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsConfigFile": "tsconfig.json"
            }
        },
        "testMatch": [
            "**/__tests__/*.spec.+(ts|tsx)"
        ]
    },
